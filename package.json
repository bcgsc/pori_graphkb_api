  "scripts": {
    "start": "node server.js",
    "import": "node --stack-size=8192  --max-old-space-size=20000 migrations/external/index.js",
    "livetest": "nyc --reporter=html --reporter=text --reporter=clover mocha -R mocha-multi-reporters --reporter-options configFile=config/coverage.json --recursive test/{routes,repo}/livedb.js --exit",
    "unit": "nyc --reporter=html --reporter=text --reporter=clover mocha -R mocha-multi-reporters --reporter-options configFile=config/coverage.json --recursive test/*/{unit,schema,query}.js",
    "test": "nyc --reporter=html --reporter=text --reporter=clover mocha -R mocha-multi-reporters --reporter-options configFile=config/coverage.json --recursive --exit --timeout 100000 test/routes/*js test/repo/{empty,query,schema,unit}.js",
    "test-ext": "nyc --reporter=html --reporter=text --reporter=clover mocha -R mocha-multi-reporters --reporter-options configFile=config/coverage.json --recursive --exit --timeout 100000 test/migrations",
    "lint": "eslint -c .eslintrc.jon app test",
    "jsdoc": "jsdoc -r app -r migrations -R README.md -d jsdoc --package package.json -c config/jsdoc.config.js",
    "version": "echo $npm_package_version"
  }
}
